# Example environment variables for local development
# Copy to .env.local and fill with real values (do NOT commit secrets)

# Database (Supabase)
DATABASE_URL="postgresql://<user>:<password>@<host>:5432/<database>"

# Supabase
SUPABASE_URL=https://<your-supabase-project>.supabase.co
SUPABASE_ANON_KEY=<supabase-anon-key>
SUPABASE_SERVICE_ROLE_KEY=<supabase-service-role-key>

# Orange SMS API
ORANGE_SMS_CLIENT_ID=<orange-client-id>
ORANGE_SMS_CLIENT_SECRET=<orange-client-secret>

# JWT / NextAuth
JWT_SECRET=<jwt-secret>
NEXTAUTH_SECRET=<nextauth-secret>
NEXTAUTH_URL=http://localhost:3000

# Note: .env.local is already in .gitignore to avoid committing secrets.
