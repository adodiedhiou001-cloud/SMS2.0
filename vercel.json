{
  "version": 2,
  "buildCommand": "npm ci && npm run build",
  "installCommand": "npm ci",
  "framework": "nextjs",
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    },
    "src/app/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "git": {
    "deploymentEnabled": {
      "master": true
    }
  },
  "env": {
    "SKIP_INSTALL_SIMPLE_GIT_HOOKS": "1",
    "NODE_ENV": "production"
  }
}
